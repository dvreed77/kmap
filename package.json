{
  "name": "hex-map",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://dvreed77.github.io/kmap",
  "scripts": {
    "build": "npm run build:css && craco build",
    "build:css": "postcss tailwind.css -o src/index.css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "start": "npm run watch:css & craco start",
    "test": "craco test --env=jsdom",
    "watch:css": "postcss tailwind.css -o src/index.css -w"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@craco/craco": "^5.6.2",
    "antd": "^3.26.2",
    "autoprefixer": "^9.7.1",
    "craco-antd": "^1.14.1",
    "d3": "^5.14.2",
    "d3-polygon": "^1.0.6",
    "d3-quadtree": "^1.0.7",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "tailwindcss": "^1.1.3",
    "transformation-matrix": "^2.2.0"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0"
  }
}
