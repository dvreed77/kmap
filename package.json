{
  "name": "hex-map",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://dvreed77.github.io/kmap",
  "scripts": {
    "build": "npm run build:css && craco build",
    "build:css": "postcss tailwind.css -o src/index.css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "start": "npm run watch:css & craco start",
    "test": "craco test --env=jsdom",
    "watch:css": "postcss tailwind.css -o src/index.css -w"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@craco/craco": "^5.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@types/ramda": "^0.26.44",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "antd": "^3.26.13",
    "autoprefixer": "^9.7.4",
    "craco-antd": "^1.14.1",
    "d3": "^5.15.0",
    "d3-polygon": "^1.0.6",
    "d3-quadtree": "^1.0.7",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "ramda": "^0.26.1",
    "react": "^16.13.0",
    "react-context-menu": "^3.0.2",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-use-dimensions": "^1.2.1",
    "tailwindcss": "^1.2.0",
    "transformation-matrix": "^2.2.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0"
  }
}
